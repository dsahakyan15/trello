{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "app/*": [
        "./src/app/*"
      ],
      "pages/*": [
        "./src/pages/*"
      ],
      "widgets/*": [
        "./src/widgets/*"
      ],
      "features/*": [
        "./src/features/*"
      ],
      "enititles/*": [
        "./src/enititles/*"
      ],
      "shared/*": [
        "./src/shared/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}